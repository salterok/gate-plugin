{
    "name": "gate-plugin",
    "displayName": "Gate plugin",
    "description": "Add gate development support to your favorite editor",
    "version": "0.1.0",
    "author": "Sergiy Samborskiy <salterok@gmail.com>",
    "publisher": "SergiySamborskiy",
    "repository": {
        "type": "git",
        "url": "https://github.com/salterok/gate-plugin.git"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "license": "MIT",
    "icon": "icon.png",
    "scripts": {
        "build-parser": "cmd /C build-parser.cmd",
        "vscode:prepublish": "npm install & npm run compile",
        "compile": "build-parser.cmd & tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "test-m": "ts-mocha -p ./tsconfig.json -u bdd -c --recursive \"tests/setup.ts\" \"tests/**/*.spec.{js,ts}\""
    },
    "dependencies": {
        "antlr4": "^4.7.1",
        "antlr4ts": "^0.5.0-alpha.8565fd2b",
        "antlr4ts-cli": "^0.4.0-canary.37504efa",
        "functional-red-black-tree": "^1.0.1",
        "lodash": "^4.17.10"
    },
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/lodash": "^4.14.116",
        "@types/mocha": "^5.2.5",
        "@types/node": "^9.4.6",
        "chai": "^4.1.2",
        "generator-code": "^1.1.25",
        "mocha": "^5.2.0",
        "ts-mocha": "^2.0.0",
        "typescript": "^2.7.2",
        "vscode": "^1.1.10"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:jape"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "jape",
                "aliases": [
                    "Jape",
                    "jape"
                ],
                "extensions": [
                    ".jape"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jape",
                "scopeName": "source.jape",
                "path": "./syntaxes/jape.tmLanguage.json",
                "embeddedLanguages": {
                    "source.java": "java"
                }
            }
        ]
    }
}
