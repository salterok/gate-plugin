{
    "name": "gate-plugin",
    "displayName": "Gate plugin",
    "description": "Add gate development support to your favorite editor",
    "version": "0.1.0",
    "author": "Sergiy Samborskiy <salterok@gmail.com>",
    "publisher": "SergiySamborskiy",
    "repository": {
        "type": "git",
        "url": "https://github.com/salterok/gate-plugin.git"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "license": "MIT",
    "icon": "icon.png",
    "scripts": {
        "build-parser": "cmd /C build-parser.cmd",
        "vscode:prepublish": "",
        "compile": "build-parser.cmd & tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
    },
    "devDependencies": {
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jape",
                "aliases": [
                    "Jape",
                    "jape"
                ],
                "extensions": [
                    ".jape"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jape",
                "scopeName": "source.jape",
                "path": "./syntaxes/jape.tmLanguage.json",
                "embeddedLanguages": {
                    "source.java": "java"
                }
            }
        ]
    }
}
